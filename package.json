{
	"name": "svelte-awesome-color-picker",
	"version": "0.0.3",
	"description": "A Svelte component to crop images with easy interactions",
	"homepage": "https://ennoriel.github.io/svelte-awesome-color-piecker",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ennoriel/svelte-awesome-color-piecker"
	},
	"author": "Maxime Dupont <m.dupont103@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"package": "svelte-kit package",
		"publish": "npm run package && npm publish ./package"
	},
	"entry": "ColorPicker.svelte",
	"devDependencies": {
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"svelte": "^3.34.0",
		"svelte-awesome-color-picker": "file:./package",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.0.0",
		"svelte2tsx": "^0.4.5",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"color picker"
	]
}
